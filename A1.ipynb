    "import numpy as np\n",
    "\n",
    "def interpolatory_quadrature_weights(x):\n",
    "    \"\"\"\n",
    "    Calculates the quadrature weights for an interpolatory quadrature rule.\n",
    "\n",
    "    Parameters:\n",
    "    x: (np.array) - Numpy array of shape (N+1,) defining quadrature points.\n",
    "\n",
    "    Returns:\n",
    "    w: (np.array) - Numpy array of shape (N+1,) defining the quadrature weights.\n",
    "    \"\"\"\n",
    "    N = len(x) - 1\n",
    "    w = np.zeros(N + 1)\n",
    "    for i in range(N + 1):\n",
    "        y = np.zeros(N + 1)\n",
    "        y[i] = 1.0\n",
    "        p = np.polyfit(x, y, N)\n",
    "        p_integ = np.polyint(p)\n",
    "        w[i] = np.polyval(p_integ, 1) - np.polyval(p_integ,-1)\n",
    "    return w\n",
    "\n",
    "# Test the function for the midpoint rule\n",
    "x_midpoint = np.array([0.0])\n",
    "w_midpoint = interpolatory_quadrature_weights(x_midpoint)\n",
    "print(\"Midpoint Rule Weights:\", w_midpoint)\n",
    "\n",
    "# Test the function for the trapezoidal rule\n",
    "x_trapezoidal = np.array([-1.0, 1.0])\n",
    "w_trapezoidal = interpolatory_quadrature_weights(x_trapezoidal)\n",
    "print(\"Trapezoidal Rule Weights:\", w_trapezoidal)\n",
    "\n",
    "# Test the function for Simpson's rule\n",
    "x_simpson = np.array([-1.0, 0.0, 1.0])\n",
    "w_simpson = interpolatory_quadrature_weights(x_simpson)\n",
    "print(\"Simpson's Rule Weights:\", w_simpson)"
