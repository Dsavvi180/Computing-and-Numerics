    "import numpy as np\n",
    "\n",
    "\n",
    "def triangle_area_kahan(a, b, c):\n",
    "    \"\"\"\n",
    "    Calculates the area of a triangle using the Kahan formula.\n",
    "\n",
    "    Parameters:\n",
    "    a, b, c - The three sides of a triangle\n",
    "\n",
    "    Returns:\n",
    "    Area of the triangle with the inputted side lengths.\n",
    "    \"\"\"\n",
    "    # Ensures higer degree of precision for floating point numbers to improve accuracy \n",
    "    a = np.longdouble(a)\n",
    "    b = np.longdouble(b)\n",
    "    c = np.longdouble(c)\n",
    "    # side lengths of triangle in decreasing order\n",
    "    a, b, c = np.array(sorted([a, b, c], reverse=True), dtype=np.longdouble)\n",
    " \n",
    "    area = np.sqrt((a+(b+c))*(c-(a-b))*(c+(a-b))*(a+(b-c)),dtype=np.longdouble)/4\n",
    "    return area"
