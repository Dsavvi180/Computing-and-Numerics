    "import numpy as np\n",
    "\n",
    "def triangle_area_heron(a, b, c):\n",
    "    \"\"\"\n",
    "    Calculates the area of a triangle using the Heron formula.\n",
    "\n",
    "    Parameters:\n",
    "    a, b, c - The three sides of a triangle\n",
    "\n",
    "    Returns:\n",
    "    Area of the triangle with the inputted side lengths.\n",
    "    \"\"\"\n",
    "    # Ensures higer degree of precision for floating point numbers to improve accuracy \n",
    "    a = np.longdouble(a)\n",
    "    b = np.longdouble(b)\n",
    "    c = np.longdouble(c)\n",
    "    a, b, c = np.array([a, b, c], dtype=np.longdouble)\n",
    "    s = ((a+b)+c)/2\n",
    "    area = np.sqrt(s*(s-a)*(s-b)*(s-c), dtype=np.longdouble)\n",
    "    return area"
   ]
