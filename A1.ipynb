    "import numpy as np\n",
    "\n",
    "A = np.array([[0,1],[1,1]])\n",
    "#creates a range of values for n\n",
    "n = np.linspace(0,50,200,dtype=int)\n",
    "# finds the eigenvalues of A and takes the one with the largest magnitude\n",
    "eigenvalues = np.linalg.eig(A)[0]\n",
    "alpha = max(eigenvalues, key=abs)\n",
    "min_eigenvalue = min(eigenvalues, key=abs) \n",
    "print(f\"Largest eigenvalue of A: {alpha}\")\n",
