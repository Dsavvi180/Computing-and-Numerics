    "import numpy as np\n",
    "\n",
    "\n",
    "def sequence_element(n):\n",
    "    \"\"\"\n",
    "    Calculates the nth term of the sequence X_(n+1)=A*X_(n)\n",
    "\n",
    "    Parameters:\n",
    "    n: (int) - Position of term in the sequence\n",
    "\n",
    "    Returns:\n",
    "    x_n: (np.array) - A 1D NumPy array with shape (2,).\n",
    "    \"\"\"\n",
    "    x_0 = np.array([1,1], dtype=int)\n",
    "    A = np.array([[0,1],[1,1]], dtype=int)\n",
    "    x_prev = x_0\n",
    "    # recursively computes the value of x_n based on the previous value of x_n\n",
    "    for _ in range(n):\n",
    "        x = A @ x_prev\n",
    "        x_prev = x \n",
    "    if n==0:\n",
    "        x=x_0\n",
    "    return x"
